{
  "name": "cocoda-2018",
  "version": "1.4.2",
  "jskos-api": "^1.0",
  "description": "Cocoda - Concordance App",
  "author": "Stefan Peters <stefandesu@exo.pm>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "npm run serve",
    "start": "npm run dev",
    "serve": "vue-cli-service serve",
    "build": "./build/build.sh",
    "build-all": "./build/build-all.sh",
    "lint": "vue-cli-service lint --no-fix",
    "fix": "vue-cli-service lint",
    "lint-staged": "lint-staged",
    "styleguide": "vue-styleguidist server",
    "styleguide:build": "vue-styleguidist build",
    "build-info": "./build/build-info.sh > ./build/build-info.json",
    "manual": "make -BC docs",
    "manual-pdf": "make -BC docs pdfs",
    "release": "./bin/release.sh",
    "kos-types": "wget http://api.dante.gbv.de/voc/top?uri=http://w3id.org/nkos/nkostype -O config/kos-types.json"
  },
  "lint-staged": {
    "**/*.{js,vue}": [
      "vue-cli-service lint"
    ],
    "**/*.json": [
      "./bin/format-json.js"
    ]
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "post-commit": "npm run build-info",
    "post-receive": "npm run build-info"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-brands-svg-icons": "^5.14.0",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@fortawesome/vue-fontawesome": "^2.0.0",
    "axios": "^0.20.0",
    "bootstrap-vue": "^2.17.3",
    "cocoda-sdk": "github:gbv/cocoda-sdk#b9ffd7b",
    "cocoda-vue-tabs": "^0.1.10",
    "css-element-queries": "^1.2.3",
    "file-saver": "^2.0.2",
    "gbv-login-client": "^0.2.10",
    "jskos-tools": "^1.0.14",
    "json-format-highlight": "^1.0.4",
    "localforage": "^1.9.0",
    "lodash": "^4.17.20",
    "vue": "^2.6.12",
    "vue-flexible-table": "^0.3.7",
    "vue-i18n": "^8.21.1",
    "vue-router": "^3.4.3",
    "vue-scrollto": "^2.18.2",
    "vuex": "^3.5.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.5.6",
    "@vue/cli-plugin-eslint": "^4.5.6",
    "@vue/cli-service": "^4.5.6",
    "@vue/eslint-config-standard": "^5.1.2",
    "@vue/test-utils": "^1.1.0",
    "babel-eslint": "^10.1.0",
    "chai": "^4.2.0",
    "copy-webpack-plugin": "^6.1.1",
    "eslint": "^7.9.0",
    "eslint-config-gbv": "github:gbv/eslint-config-gbv#ecaf810",
    "eslint-plugin-vue": "^6.2.2",
    "glob": "^7.1.6",
    "jsdoc-to-markdown": "^6.0.1",
    "less": "^3.12.2",
    "less-loader": "^7.0.1",
    "lint-staged": "^10.4.0",
    "vue-styleguidist": "^4.32.3",
    "vue-template-compiler": "^2.6.12"
  },
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=6.8.0"
  }
}
