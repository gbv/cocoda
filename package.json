{
  "name": "cocoda-2018",
  "version": "1.4.4",
  "jskos-api": "^1.0",
  "description": "Cocoda - Concordance App",
  "author": "Stefan Peters <stefandesu@exo.pm>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "npm run serve",
    "start": "npm run dev",
    "serve": "vue-cli-service serve",
    "build": "./build/build.sh",
    "build-all": "./build/build-all.sh",
    "lint": "vue-cli-service lint --no-fix",
    "fix": "vue-cli-service lint",
    "lint-staged": "lint-staged",
    "styleguide": "vue-styleguidist server",
    "styleguide:build": "vue-styleguidist build",
    "build-info": "./build/build-info.sh > ./build/build-info.json",
    "manual": "make -BC docs",
    "manual-pdf": "make -BC docs pdfs",
    "release": "./bin/release.sh",
    "kos-types": "wget http://api.dante.gbv.de/voc/top?uri=http://w3id.org/nkos/nkostype -O config/kos-types.json"
  },
  "lint-staged": {
    "**/*.{js,vue}": [
      "vue-cli-service lint"
    ],
    "**/*.json": [
      "./bin/format-json.js"
    ]
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "post-commit": "npm run build-info",
    "post-receive": "npm run build-info"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.34",
    "@fortawesome/free-brands-svg-icons": "^5.15.2",
    "@fortawesome/free-solid-svg-icons": "^5.15.2",
    "@fortawesome/vue-fontawesome": "^2.0.2",
    "axios": "^0.21.1",
    "bootstrap-vue": "^2.21.2",
    "cocoda-sdk": "^1.0.10",
    "cocoda-vue-tabs": "^0.1.10",
    "css-element-queries": "^1.2.3",
    "file-saver": "^2.0.5",
    "gbv-login-client": "^0.2.13",
    "jskos-tools": "^1.0.16",
    "json-format-highlight": "^1.0.4",
    "localforage": "^1.9.0",
    "lodash": "^4.17.20",
    "vue": "^2.6.12",
    "vue-flexible-table": "^0.3.7",
    "vue-i18n": "^8.22.4",
    "vue-router": "^3.5.1",
    "vue-scrollto": "^2.20.0",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.5.11",
    "@vue/cli-plugin-eslint": "^4.5.11",
    "@vue/cli-service": "^4.5.11",
    "@vue/eslint-config-standard": "^5.1.2",
    "@vue/test-utils": "^1.1.2",
    "babel-eslint": "^10.1.0",
    "chai": "^4.2.0",
    "copy-webpack-plugin": "^6.4.1",
    "eslint": "^7.19.0",
    "eslint-config-gbv": "^1.0.1",
    "eslint-plugin-vue": "^6.2.2",
    "glob": "^7.1.6",
    "jsdoc-to-markdown": "^6.0.1",
    "less": "^3.13.1",
    "less-loader": "^7.3.0",
    "lint-staged": "^10.5.3",
    "vue-styleguidist": "^4.35.0",
    "vue-template-compiler": "^2.6.12"
  },
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=6.8.0"
  }
}
