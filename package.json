{
  "name": "cocoda-2018",
  "version": "1.2.0",
  "jskos-api": "^1.0",
  "description": "Cocoda - Concordance App",
  "author": "Stefan Peters <stefandesu@exo.pm>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "npm run serve",
    "start": "npm run dev",
    "serve": "vue-cli-service serve",
    "build": "./build/build.sh",
    "build-all": "./build/build-all.sh",
    "lint": "vue-cli-service lint --no-fix",
    "fix": "vue-cli-service lint",
    "lint-staged": "lint-staged",
    "styleguide": "vue-styleguidist server",
    "styleguide:build": "vue-styleguidist build",
    "build-info": "./build/build-info.sh > ./build/build-info.json",
    "manual": "make -BC docs",
    "manual-pdf": "make -BC docs pdfs",
    "release": "./bin/release.sh",
    "kos-types": "wget http://api.dante.gbv.de/voc/top?uri=http://w3id.org/nkos/nkostype -O config/kos-types.json"
  },
  "lint-staged": {
    "**/*.{js,vue}": [
      "vue-cli-service lint",
      "git add"
    ],
    "**/*.json": [
      "./bin/format-json.js",
      "git add"
    ]
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "post-commit": "npm run build-info",
    "post-receive": "npm run build-info"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.26",
    "@fortawesome/free-brands-svg-icons": "^5.12.0",
    "@fortawesome/free-solid-svg-icons": "^5.12.0",
    "@fortawesome/vue-fontawesome": "^0.1.9",
    "axios": "^0.19.0",
    "bootstrap-vue": "2.0.0-rc.19",
    "cocoda-vue-tabs": "^0.1.10",
    "css-element-queries": "^1.2.3",
    "file-saver": "^2.0.1",
    "gbv-login-client": "^0.2.6",
    "jquery": "^3.4.0",
    "jskos-tools": "^1.0.2",
    "json-format-highlight": "^1.0.3",
    "localforage": "^1.7.3",
    "lodash": "^4.17.15",
    "qs": "^6.9.1",
    "simple-sha1": "^3.0.1",
    "uuid": "^3.3.2",
    "vue": "^2.6.11",
    "vue-flexible-table": "^0.3.3",
    "vue-i18n": "^8.15.3",
    "vue-router": "^3.1.3",
    "vue-scrollto": "^2.17.1",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.12.1",
    "@vue/cli-plugin-eslint": "^3.12.1",
    "@vue/cli-service": "^4.1.2",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-eslint": "^10.0.1",
    "chai": "^4.1.2",
    "copy-webpack-plugin": "^5.1.1",
    "eslint": "^6.8.0",
    "eslint-config-gbv": "git+https://github.com/gbv/eslint-config-gbv.git",
    "eslint-plugin-vue": "^5.0.0",
    "glob": "^7.1.6",
    "jsdoc-to-markdown": "^5.0.3",
    "less": "^3.0.4",
    "less-loader": "^5.0.0",
    "lint-staged": "^9.5.0",
    "vue-styleguidist": "^4.2.3",
    "vue-template-compiler": "^2.6.11"
  },
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=6.8.0"
  }
}
